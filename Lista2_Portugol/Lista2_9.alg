algoritmo "Lista2_9"
var
peso:inteiro
grama,novop: real
inicio
escreval("Conversão de peso em gramas")
escreval("Digite peso em Kilos")
leia(peso)
grama<-(peso*1000)
novop<-((peso*0.12) + peso)*1000
escreval("a - Seu peso convertido em gramas é: ",grama, "g")
escreval("b - Seu novo peso é: ",novop)
fimalgoritmo
