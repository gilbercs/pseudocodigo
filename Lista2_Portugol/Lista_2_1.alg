algoritmo "lista_2_1"
var
n1,n2:inteiro
inicio
// Seção de Comandos 
escreva("Digite um numero:")
leia(n1)
escreva("Digite outro valor:")
leia(n2)
se (n1>n2) entao
escreval(" O maior número é: ",n1)
escreval(" O menor número é: ",n2)
senao
escreval("1 - O maior número é: ",n2)
escreval("2 - O menor número é: ",n1)
fimse
fimalgoritmo
